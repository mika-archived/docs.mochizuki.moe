{
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/en-us/(.*)", "destination": "/index.md" },
    { "source": "/ja-jp/(.*)", "destination": "/index.md" }
  ]
}
